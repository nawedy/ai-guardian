{
  "projects": [
    {
      "source": "frontend/web-dashboard/web-dashboard",
      "outputDirectory": "frontend/web-dashboard/web-dashboard/dist"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "installCommand": "cd frontend/web-dashboard/web-dashboard && pnpm install",
  "buildCommand": "cd frontend/web-dashboard/web-dashboard && pnpm run build",
  "framework": "vite"
} 